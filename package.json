{
  "name": "ai-credit-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "echo 'No tests configured yet'",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "analyze:bundle": "cross-env ANALYZE=true next build",
    "check:all": "npm run type-check && npm run lint && npm run format:check",
    "fix:all": "npm run lint:fix && npm run format",
    "precommit": "npm run check:all",
    "postinstall": "echo 'Setup complete!'",
    "vercel:build": "npm run build",
    "vercel:deploy": "vercel --prod",
    "docker:build": "docker build -t ai-cost-guardian .",
    "docker:run": "docker run -p 3000:3000 ai-cost-guardian",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@google/generative-ai": "^0.24.1",
    "@lobehub/icons": "^1.30.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "antd": "^5.26.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.536.0",
    "next": "14.2.5",
    "next-auth": "^4.24.11",
    "openai": "^5.11.0",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.19",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
